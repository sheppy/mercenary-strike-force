{
    "bitwise": true,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "freeze": true,
    "funcscope": false,
    "globalstrict": true,
    "immed": true,
    "indent": 4,
    "iterator": false,
    "latedef": true,
    "maxcomplexity": 8,
    "maxdepth": 2,
    "maxerr": 50,
    "maxlen": 100,
    "maxparams": 3,
    "maxstatements": 8,
    "newcap": true,
    "noarg": true,
    "nocomma": false,
    "noempty": true,
    "nonbsp": true,
    "nonew": true,
    "notypeof": false,
    "quotmark": "double",
    "shadow": "outer",
    "singleGroups": true,
    "undef": true,
    "unused": true,
    "browser": true,
    "strict": false,
    "esnext": true,
    "globals": {
        "PIXI": false,
        "FPSMeter": false
    }
}
